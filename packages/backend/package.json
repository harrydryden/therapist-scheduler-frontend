{
  "name": "therapist-scheduler-backend",
  "version": "1.0.0",
  "description": "Node.js backend for therapist scheduling platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "prisma generate && tsc",
    "postinstall": "prisma generate",
    "start": "sh prisma/baseline.sh && node dist/server.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "migrate:conversation-state": "tsx src/scripts/migrate-conversation-state.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "test": "jest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@fastify/compress": "^8.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/multipart": "^8.1.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/redis": "^6.1.1",
    "@fastify/static": "^9.0.0",
    "@fastify/websocket": "^10.0.1",
    "@notionhq/client": "^2.2.14",
    "@prisma/client": "^5.9.0",
    "@therapist-scheduler/shared": "*",
    "chrono-node": "^2.9.0",
    "fastify": "^4.25.2",
    "google-auth-library": "^10.5.0",
    "googleapis": "^170.1.0",
    "bullmq": "^5.34.8",
    "ioredis": "^5.3.2",
    "pdf-parse": "^1.1.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@railway/cli": "^4.27.5",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prisma": "^5.9.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
